
[client-server.#NODE_SECTION#]
port                    = 33#NODE_ID#6
socket                  = /run/mysqld/#NODE#.sock
[mariadb.#NODE_SECTION#]
user                    = mysql
pid-file                = /run/mysqld/#NODE#.pid
datadir                 = /var/lib/mysql-multi/#NODE#
log_bin                 = /var/log/mysql-multi/#NODE#/mysql-bin.log
log_error               = /var/log/mysql-multi/#NODE#/error.log
log-basename            = #NODE#
server-id               = 1#NODE_ID#
#gtid-domain-id          = 1
tmpdir                  = /tmp-mysql:/tmp
slave_load_tmpdir       = /tmp
#replica_load_tmpdir    = /tmp
